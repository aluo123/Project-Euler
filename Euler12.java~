public class Euler12 {
  public static void main(String [] args) {
  }
  
  int numDivisors( int n ) {
    int divisors = 1;
    int prime = 2;
    int occurrence = 1;
    while ( n != 1 ) {
      if ( n % prime == 0 ) {
        occurence++;
      }
      else {
        divisors *= occurrence;
        prime++;
        occurrence = 1;
      }
    }
    return divisors;
}